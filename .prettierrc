{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 110,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "useTabs": false,
  "trailingComma": "es5",
  "endOfLine": "auto",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)",
    "^next(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@(/.*)",
    "^@/layouts(/.*)?$",
    "^@/components(/.*)?$",
    "^@/libs/primitives(/.*)?$",
    "^@/libs/providers(/.*)?$",
    "^@/libs/hooks(/.*)?$",
    "^@/libs/methods(/.*)?$",
    "^@/theme(/.*)?$",
    "^@/constants(/.*)?$",
    "^@/assets(/.*)?$",
    "^@/types(/.*)?$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
